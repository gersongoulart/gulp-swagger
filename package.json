	"main": "./index.js",
	"repository": {
		"type": "git",
		"url": "git://github.com/gersongoulart/gulp-swagger.git"
	},
	"dependencies": {
		"gulp-util": "^3.0.7",
		"swagger-js-codegen": "git+ssh://git@github.com/yann-stepienik-cko/swagger-js-codegen.git",
		"swagger-parser": "^3.4.1",
		"swagger-tools": "^0.10.1",
		"through2": "^2.0.1"
